      <div class="card-header text-center">
        <h1>Invoice</h1>
        <% if(orders){ %>
        <div class="date">
          Date: <span> <%= currentDate.toLocaleDateString('en-US', { weekday: 'short' ,month: 'short' , day: 'numeric' , year:'numeric'})  %></span>
        </div>
      </div>
      <div class="d-flex justify-content-between">
        <div class="address-details">
          <h6>Shipping Address</h6>
          <p>
            <% Object.entries(orders.shippingAddress).forEach(([key, value]) => { %>
            <strong><%= key.charAt(0).toUpperCase() + key.slice(1) %>:</strong> <%= value %><br>
            <% }); %>
          </p>
        </div>

        <div>
          <p><strong>Order Date</strong> <%= orders.orderDate.toLocaleDateString('en-US', { weekday: 'short' ,month: 'short' , day: 'numeric' , year:'numeric'}) %></p>
        </div>
      </div>


      <div class="card-body">
        <table class="table table-striped invoiceTable">
          <thead>
            <tr>
              <th>Item</th>
              <th>Quantity</th>
              <th>Price</th>
              <th>Offer Discount</th>
              <th>Coupon Discount</th>
              <th>Status</th>
              <th>Total</th>
            </tr>
          </thead>
          <tbody>
           

            <% completedOrder.forEach((item)=>{ %>
         

            <tr>
              <td><%= item.orderItems.variantName %></td>
              <td><%= item.orderItems.quantity %></td>
              <td><%= item.orderItems.variantPrice %></td>
              <td><%= item.orderItems.offerDiscount %></td>
              <td><%= couponAppliedForEachProduct %></td>
              <td><%= item.orderStatus %></td>
              <td><%= item.orderItems.variantPrice -item.orderItems.offerDiscount - couponAppliedForEachProduct %></td>
            </tr>
            <% }) %>

          </tbody>
        </table>
      </div>

      <div class="card-footer text-right">
        <div class="total-amount mb-3">

          <div>
            <p id="subTotal"><strong>Sub Total : </strong><%= subTotal %></p>
            <p id="deliveryCharge"><strong>Delivery charge : </strong><%= orders.deliveryCharge %></p>
            <p id="totalOfferDiscount"><strong>Total offer discount : </strong><%= totalOfferDiscount %></p>
            <p id="couponDis"><strong>Coupon discount : </strong>
              <% if (couponDis > 0) { %>
              <%= couponDis %>
              <% } else { %>
              None
              <% } %>
            </p>
            <p id="grandTotal"><strong> Grand Total : </strong><%= grandTotal %></p>
          </div>

          <button class="btn btn-primary" onclick="downloadInvoice()">Download PDF</button>

        </div>
      </div>
  </div>
