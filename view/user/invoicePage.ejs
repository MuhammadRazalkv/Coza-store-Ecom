  <script src="https://cdnjs.cloudflare.com/ajax/libs/jspdf-autotable/3.5.21/jspdf.plugin.autotable.min.js"></script>
  <script>
    let data = [];
    let orders = <%- JSON.stringify(orders) %>;

    document.addEventListener('DOMContentLoaded', function () {
        console.log('DOM content loaded');
        document.querySelectorAll('.invoiceTable tbody tr').forEach(row => {
            console.log('Inside the loop');
            let rowData = [];
            row.querySelectorAll('td').forEach(cell => {
                rowData.push(cell.textContent.trim());
            });
            data.push(rowData);
        });
        console.log('data: ', data);
    });

    window.downloadInvoice = function () {
        const { jsPDF } = window.jspdf;
        const doc = new jsPDF();

        // Define margin and initial Y position
        const margin = 14;
        let yPos = margin;

        // Title and Company Information
        doc.setFontSize(18);
        doc.text('CozaStore Invoice', margin, yPos);
        doc.setFontSize(10);
        yPos += 10;
        doc.text('CozaStore', margin, yPos);
        doc.text('1234 Main Street', margin, yPos + 5);
        doc.text('City, State, ZIP', margin, yPos + 10);
        doc.text('Phone: (123) 456-7890', margin, yPos + 15);
        yPos += 25;

        // Shipping Address and Order Date
        doc.setFontSize(10);
        const shippingAddress = [
            'Shipping Address:',
            ...Object.entries(orders.shippingAddress).map(([key, value]) => `${key.charAt(0).toUpperCase() + key.slice(1)}: ${value}`)
        ].join('\n');
        doc.text(shippingAddress, margin, yPos);

        const orderDate = `Order Date: ${new Date(orders.orderDate).toLocaleDateString('en-US', {
            weekday: 'short',
            month: 'short',
            day: 'numeric',
            year: 'numeric'
        })}`;
        doc.text(orderDate, margin + 100, yPos);
        yPos += 30;

        // Order Summary
        const orderSummaryYPos = yPos; // Store the Y position for order summary

        // Order summary on the right
        doc.setFontSize(10);
        const orderSummary = [
            `Sub Total: ${document.getElementById('subTotal').textContent.split(':')[1].trim()}`,
            `Delivery Charge: ${document.getElementById('deliveryCharge').textContent.split(':')[1].trim()}`,
            `Total Offer Discount: ${document.getElementById('totalOfferDiscount').textContent.split(':')[1].trim()}`,
            `Coupon Discount: ${document.getElementById('couponDis').textContent.split(':')[1].trim()}`,
            `Grand Total: ${document.getElementById('grandTotal').textContent.split(':')[1].trim()}`
        ];

        orderSummary.forEach((detail, index) => {
            doc.text(detail, margin + 100, orderSummaryYPos + (index * 6));
        });

        yPos += Math.max(orderSummary.length * 6, 30); // Ensure enough space between sections

        // Itemized Table
        const columns = [
            'Item', 'Quantity', 'Price', 'Offer Discount', 'Coupon Discount', 'Status', 'Total'
        ];

        doc.autoTable({
            head: [columns],
            body: data,
            startY: yPos,
            theme: 'striped',
            styles: {
                cellPadding: 1,
                fontSize: 10,
                halign: 'center',
                valign: 'middle'
            },
            headStyles: {
                fillColor: [22, 160, 133],
                textColor: [255, 255, 255],
                fontStyle: 'bold'
            },
            margin: { top: yPos }
        });

        
        yPos = doc.previousAutoTable.finalY + 10;
        doc.setFontSize(8);
        doc.text('Thank you for your business!', margin, yPos);
        doc.text('If you have any questions, feel free to contact us at support@cozastore.com', margin, yPos + 5);

        doc.save('invoice.pdf');
    };
</script>

