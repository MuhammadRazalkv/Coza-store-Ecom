							<div class="profile-section address-section addresses">
								<% if (addressData && addressData.addresses && addressData.addresses.length > 0) { %>
									
										<% addressData.addresses.forEach((address, index) => { %>
											<!-- <input type="hidden" value="<%= address._id %>" id="addressId<%= index %>>"> -->
											<div class="mb-4">
												<h5 class="mb-3">Address <%= index + 1 %></h5>
												<div>
													<label>
														<input type="radio" name="selectedAddress" value="<%= address._id %>" <% if (index === 0) { %>checked<% } %>>
														<strong>Name:</strong> <%= address.name %><br>
														<strong>Phone:</strong> <%= address.altPhone %><br>
														<strong>Locality:</strong> <%= address.locality %><br>
														<strong>Address:</strong> <%= address.address %><br>
														<strong>City:</strong> <%= address.city %><br>
														<strong>State:</strong> <%= address.state %><br>
														<strong>PIN:</strong> <%= address.pinCode %><br>
														<strong>Landmark:</strong> <%= address.landmark %><br>
														<strong>Type:</strong> <%= address.addressType %><br>
													</label>
												</div>
											</div>
										<% }); %>
										
									
								
									<!-- <p>No address found, add a new one</p>
									<div class="mt-4">
										<a href="/myAccount" class="btn btn-primary">Add New Address</a>
									</div> -->
								<% } %>
								<button class="btn btn-primary btn-sm" id="differentAddress" data-toggle="modal" data-target="#addressModal">Ship to a different address</button>
							</div>
						</div>
					</div>
				</div>

				<!-- Address modal  -->
				<div class="modal fade" id="addressModal">
					<div class="modal-dialog modal-lg">
						<div class="modal-content">
				
							<!-- Modal Header -->
							<div class="modal-header">
								<h4 class="modal-title">Add Your Address</h4>
								<button type="button" class="close" data-dismiss="modal">&times;</button>
							</div>
				
							<!-- Modal Body -->
							<div class="modal-body">
								<form id="addressForm" novalidate>
									<div class="form-group">
										<label for="firstname">Name:</label>
										<input type="text" class="form-control" id="firstname" name="userName" pattern="^[A-Za-z\s]+$" required>
										<div class="invalid-feedback">Please enter a valid name (letters and spaces only).</div>
									</div>
				
									<div class="form-group">
										<label for="locality">Locality:</label>
										<input type="text" class="form-control" id="locality" name="userLocality" required>
										<div class="invalid-feedback">Please enter your locality.</div>
									</div>
				
									<div class="form-group">
										<label for="userphone">Phone:</label>
										<input type="text" class="form-control" id="userphone" name="userAltPhone" pattern="^\d{10}$" required>
										<div class="invalid-feedback">Phone number must be 10 digits.</div>
									</div>
				
									<div class="form-group">
										<label for="useraddress">Address:</label>
										<input type="text" class="form-control" id="useraddress" name="userAddress" required>
										<div class="invalid-feedback">Please enter a valid address.</div>
									</div>
				
									<div class="form-group">
										<label for="userLandmark">Landmark:</label>
										<input type="text" class="form-control" id="userLandmark" name="userLandmark" pattern="^[A-Za-z0-9\s]+$" required>
										<div class="invalid-feedback">Please enter a valid landmark (No special characters allowed).</div>
									</div>
				
									<div class="form-group">
										<label for="userCity">City:</label>
										<input type="text" class="form-control" id="userCity" name="userCity" pattern="^[A-Za-z\s]+$" required>
										<div class="invalid-feedback">Please enter your city (letters and spaces only).</div>
									</div>
				
									<div class="form-group">
										<label for="userState">State:</label>
										<input type="text" class="form-control" id="userState" pattern="^[A-Za-z\s]+$" name="userState" required>
										<div class="invalid-feedback">Please enter your State (letters and spaces only).</div>
									</div>
				
									<div class="form-group">
										<label for="userPIN">PIN:</label>
										<input type="text" class="form-control" id="userPIN" name="userPIN" pattern="^\d{6}$" required>
										<div class="invalid-feedback">Please enter a valid PIN (6 digits).</div>
									</div>
			
									<div class="form-group">
										<label for="addressType">Type:</label>
										<select class="form-control" id="addressType" name="addressType" required>
											<option value="">Select Address Type</option> <!-- Add this option to enforce selection -->
											<option value="Home">Home</option>
											<option value="Work">Work</option>
										</select>
										<div class="invalid-feedback">Please select an address type.</div>
									</div>
				
									<button type="submit" class="btn btn-primary" style="height: 35px; width: fit-content;">Add Address</button>
								</form>
							</div>
						</div>
					</div>
				</div>
				

				
				
