			/* Basic styling for the payment options */
.payment-option {
    background-color: #f8f9fa;
    border: 1px solid #dee2e6;
    border-radius: 0.25rem;
    transition: all 0.3s ease-in-out;
    margin-bottom: 15px;
}

.payment-option:hover {
    box-shadow: 0px 0px 10px rgba(0, 0, 0, 0.1);
}

.payment-option .h6 a {
    color: #343a40;
    text-decoration: none;
}

.payment-option .h6 a:hover {
    text-decoration: underline;
}

.payment-option .collapse {
    transition: height 0.3s ease-in-out;
}

/* Styling for the expanded (selected) state */
.payment-option.show {
    border-color: #007bff;
    box-shadow: 0px 0px 10px rgba(0, 123, 255, 0.3);
}

.payment-option.show .h6 a {
    color: #007bff;
}

.payment-option.show:hover {
    box-shadow: 0px 0px 10px rgba(0, 123, 255, 0.5);
}

.payment-option.show .collapse {
    height: auto;
}
.payment-option {
        cursor: pointer; /* Change cursor to pointer to indicate clickable */
        transition: background-color 0.3s ease; /* Smooth transition for background color */
    }

    .payment-option.selected {
        background-color: #949ea7; /* Background color when selected */
        border-color: #007bff; /* Border color when selected */
    }

    .payment-option.selected a {
        font-weight: bold; /* Bold text when selected */
    }

