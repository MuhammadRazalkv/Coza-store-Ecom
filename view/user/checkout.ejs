<!-- ----------------- Modal for showing coupons --------------------- -->


<div class="modal fade " id="couponListModal" tabindex="-1" role="dialog" aria-labelledby="couponListModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="couponListModalLabel">Available Coupons</h5>
                <button type="button" class="close" id="closeModalBtn" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div id="couponList" class="row">
                    <!-- Dynamic coupon cards -->
                    <% if(coupons && coupons.length > 0) { %>
                        <% coupons.forEach((coupon,index) => { %>
                            <div class="col-lg-6 col-md-6 col-sm-12 mb-4">
                                <div class="card cm-card">
                                    <div class="card-body">
                                        <h4 class="card-title"><%= coupon.couponName %></h4>
										<div class="d-flex justify-content-between">
											<h6 class="card-subtitle mb-2 text-muted" >Code:</h6>
											<h6 class="card-subtitle mb-2 text-muted" id="code<%= index %>"> <%= coupon.couponCode %></h6>

											
											<button  onclick="copyText('<%= index %>')" class="btn btn-sm btn-info">Copy</button>
										</div>

                                      
                                        <p class="card-text">Discount: <%= coupon.discountPercentage %>%</p>
                                        <p class="card-text">Minimum Purchase Amount: <%= coupon.minPurchaseAmount %></p>
                                        <p class="card-text">Max Redeem Amount: <%= coupon.maxRedeemAmount %></p>
                                        <p class="card-text">Expiry Date: <%= coupon.expiryDate.toLocaleDateString('en-US', { weekday: 'short', month: 'short', day: 'numeric', year: 'numeric' }) %></p>
                                    </div>
                                </div>
                            </div>
                        <% }); %>
                    <% } else { %>
                        <div class="col-12">
                            <p>No coupons found.</p>
                        </div>
                    <% } %>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" id="modalCloseBtn">Close</button>
            </div>
        </div>
    </div>
</div>



